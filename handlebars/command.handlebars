const BaseCommand = require("./base/BaseCommand");
{{#if permissions}}
const { permissionsEnum } = require("@beanc16/discordjs-helpers");
{{/if}}



class {{capital name}} extends BaseCommand
{
    async run({
        args,
        attachments,
        bot,
        channel,
        helpers: {
            allBotCommandNames,
            allBotCommandAbbreviations,
            botHasAbbreviation,
            botHasCommand,
            botGetCommand,
        },
        prefix,
        message,
        reactions,
        server,
        user,
    })
    {
        throw new Error(`${this.commandName} has not yet been implemented!`);
    }
    {{! Include whitespace if abbreviations or permissions exist}}
    {{#ifCond abbreviations "||" permissions}}



    {{/ifCond}}
    {{#if abbreviations}}
    get abbreviations()
    {
        return [
        {{#each abbreviations}}
            "{{this}}",
        {{/each}}
        ];
    }
    {{/if}}
    {{#if permissions}}
    {{! Include whitespace if abbreviations exists above this}}
    {{#if abbreviations}}

    {{/if}}
    get requiredPermissions()
    {
        return [
        {{#each permissions}}
            {{this}},
        {{/each}}
        ];
    }
    {{/if}}



    /*
     * Help documentation
     */

    get description()
    {
        return `{{description}}`;
    }
    {{#if examples}}

    get examples()
    {
        return [
        {{#each examples}}
            `{{this}}`,
        {{/each}}
        ];
    }
    {{/if}}
}



module.exports = new {{capital name}}();
