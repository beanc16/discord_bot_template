{
	"name": "template_bot",
	"version": "1.0.0",
	"description": "A discord bot template for developers to clone so that they can start programming on top of a bot with basic functionality.",
	"main": "bot.js",
	"author": "ShockingShots",
	"scripts": {
		"start": "node bot.js",
		"start-dev": "nodemon bot.js",
		"debug-dependencies": "node dev_tools/madge.js",
		"generate-new-command": "node generators/generateNewCommand.js",
		"run-first-time-setup": "node generators/runFirstTimeSetup.js",
		"version": "git add -A",
		"postversion": "git push && git push --tags"
	},
	"dependencies": {
		"@beanc16/discordjs-helpers": "^1.3.0",
		"@beanc16/logger": "^1.3.1",
		"@beanc16/microservices-abstraction": "^1.3.1",
		"discord.js": "^12.2.0",
		"dotenv": "^16.0.1",
		"prompt": "^1.0.0"
	},
	"devDependencies": {
		"madge": "^5.0.1",
		"nodemon": "^2.0.18"
	}
}
